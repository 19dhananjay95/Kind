App.Kaelin.App.draw(
  img: VoxBox
): App.Draw<App.Kaelin.State>

(state)
  open state
  stage = state.global@stage
  case stage {
    init: App.Kaelin.Draw.init
    draft: 
      App.Kaelin.Draw.draft(stage.players, stage.coords, state.local@user)
    planning: DOM.vbox({}, {}, App.Kaelin.Draw.state(img, state))
  } default DOM.text("Em breve")
