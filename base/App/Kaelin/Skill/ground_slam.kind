App.Kaelin.Skill.ground_slam(
  key: U16
  dmg: I32
): App.Kaelin.Effect<Unit>

  App.Kaelin.Effect { 
    get center = App.Kaelin.Effect.coord.get_center
    let area = App.Kaelin.Coord.range(center, 2)
    App.Kaelin.Effect.ap.cost(key, center)
    App.Kaelin.Effect.area!(App.Kaelin.Effect.hp.damage(dmg), area)
    return unit
  }
