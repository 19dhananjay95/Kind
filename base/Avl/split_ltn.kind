Avl.split_ltn<K: Type, V: Type>(cmp: K -> K-> Cmp, cut: K, map: Avl<K,V>): Avl<K,V>
  case map {
    tip: map,
    bin: case cmp(cut, map.key) {
      ltn: Avl.split_ltn<K,V>(cmp, cut, map.left),
      eql: map.left,
      gtn:  
        let key   = map.key 
        let val   = map.val 
        let left  = map.left 
        let right = Avl.split_ltn<K,V>(cmp, cut, map.right)
        Avl.concat3<K,V>(cmp, key, val, left, right)
    }
  }