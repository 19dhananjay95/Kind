. inc_test
  [F = (n2 #[x]{x + 1})]
  # (F 0)

. get_fst_char_test
  [F = ("test" #cons)]
  # {(head 0 (tail (F nil))) >> 24}

. incs_test
  [F = (n256 #[x]{x + 1})]
  # (F 0)

. main
  get_fst_char_test

