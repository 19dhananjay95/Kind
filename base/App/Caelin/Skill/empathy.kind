App.Kaelin.Skill.empathy(
  key: U16
  dmg: I32
  range: I32
): App.Kaelin.Effect<Unit>

  App.Kaelin.Effect { 
    get map = App.Kaelin.Effect.map.get
    get target = App.Kaelin.Effect.coord.get_target
    get center = App.Kaelin.Effect.coord.get_center
    let area = App.Kaelin.Coord.range(target, range)
    App.Kaelin.Effect.ap.cost(key, center)
    App.Kaelin.Effect.hp.damage_at(dmg, center)
    App.Kaelin.Effect.area!(App.Kaelin.Effect.hp.heal(dmg*2), area)
    return unit
  }
