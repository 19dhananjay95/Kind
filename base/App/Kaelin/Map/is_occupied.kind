// Checks if a tile is occupied by a creature
App.Kaelin.Map.is_occupied(
  coord: App.Kaelin.Coord,
  map: App.Kaelin.Map
): Bool

maybe_tile = App.Kaelin.Map.get(coord, map)
case maybe_tile { 
  none: false
  some:
    open maybe_tile.value as tile
    case tile.creature { 
      none: false
      some: true
    }
}
