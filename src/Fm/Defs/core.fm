Fm.Defs.core(defs: Fm.Defs): String
  let result = "";
  for defn in Map.values<_>(defs) with result:
    case defn {
      new: 
        case defn.stat {
          done:
            let name = defn.name;
            let term = Fm.Core.show(defn.term);
            let type = Fm.Core.show(defn.type);
            String.flatten([result, name, " : ", type, " = ", term, ";\n"]),
          wait: result,
          init: result,
          fail: result,
      }
    }
