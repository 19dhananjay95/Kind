// function to draw according the state
App.Kaelin.Draw.state(img: VoxBox, state: App.Store<App.Kaelin.State>): VoxBox
  open state
  case state.global state.local {
      game game:
        let img = App.Kaelin.Draw.state.map(state.global.map, state.local.cast_info, state.local.env_info, state.global.internal, img)
        let img = App.Kaelin.Draw.state.mouse_ui(state.local.env_info, img)
        img
  } default img



