// move by x and y
// this function will change when the state change and the click event came
// waiting for click to be ready
Web.Kaelin.Action.move(interface : App.EnvInfo, state: Web.Kaelin.State): Web.Kaelin.State
   case state {
     game: 
        Web.Kaelin.State.game(state.room, state.tick, state.players, state.map, interface)
   } default state
