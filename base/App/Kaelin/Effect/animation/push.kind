App.Kaelin.Effect.animation.push
  (coords : List<App.Kaelin.Coord>
): App.Kaelin.Effect<Unit>

  (center, target, map)
  map = List.foldr!!(map, 
    (coord, map) 
      App.Kaelin.Map.push(coord, App.Kaelin.Map.Entity.animation(App.Kaelin.Animation.new(some(45), App.Kaelin.Sprite.fire)),
     map), coords)
  App.Kaelin.Effect.Result.new<Unit>(Unit.new, map, [], NatMap.new!)
