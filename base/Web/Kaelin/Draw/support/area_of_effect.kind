Web.Kaelin.Draw.support.area_of_effect(
  cast_info: Web.Kaelin.CastInfo,
  coord: Web.Kaelin.Coord,
  coord_nat: Nat,
  area: NatMap(Web.Kaelin.Indicator)
  ): Maybe(Web.Kaelin.Indicator)
    open cast_info
      case cast_info.hex_effect {
        skill:
        mouse_pos = Web.Kaelin.Coord.Convert.axial_to_nat(cast_info.mouse_pos)
        if NatSet.has(mouse_pos, cast_info.range) then
          NatMap.get!(coord_nat,area)
        else
          none 
      } default none

