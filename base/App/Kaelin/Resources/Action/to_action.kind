App.Kaelin.Resources.Action.to_action: App.Kaelin.Event.Buffer<App.Kaelin.Action>
  def return = App.Kaelin.Event.Buffer.monad.pure
  App.Kaelin.Event.Buffer {
    get id = App.Kaelin.Event.Buffer.next
    switch Nat.eql(id) {
      0 : return!(App.Kaelin.Action.walk)
      1 : return!(App.Kaelin.Action.ability_0)
      2 : return!(App.Kaelin.Action.ability_1)
    } default App.Kaelin.Event.Buffer.fail!
  } 