Kind.Comp.Prim.of(type: Kind.Term, defs: Kind.Defs): Maybe(Kind.Comp.Prim)
  let result = none
  for prim in Kind.Comp.Prim.natives with result:
    open prim
    case result {
      none:
        if Kind.Term.equal(type, Kind.Term.ref(prim.fst), defs) then
          some(prim.snd)
        else
          none
      some: result
    }
  result
