Kind.Parser.letter: Parser(Kind.Letter)
  (long, idx, code) case code {
    nil: Parser.Reply.error_long!(idx, code, "Unexpected eof."),
    cons:
      if Kind.Name.is_letter(code.head) then
        Parser.Reply.value!(long, Nat.succ(idx), code.tail, code.head)
      else
        Parser.Reply.error_long!(idx, code, "Expected letter."),
  }
