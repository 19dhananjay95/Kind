App.KL.Game.Hero.Lela.hero: App.KL.Game.Hero
  let name = "Lela"

  let draw = (player)
    App.KL.Game.Hero.Lela.Assets.vbox_idle

  let picture = (bool, time)
    App.KL.Game.Hero.Lela.Assets.base64_idle

  let max_hp = 18 :: I32

  let max_ap = 3 :: I32

  let skills = {
    "X": App.KL.Game.Skill.new("Walk", 500, 4, 2, App.KL.Game.Effect.common.walk(2, 1))
    "Q": App.KL.Game.Skill.new("Restore", 398, 16, 4, App.KL.Game.Hero.Lela.hero.Skills.restore(4, 2, 1))
    "W": App.KL.Game.Skill.new("Escort", 403, 16, 4, App.KL.Game.Hero.Lela.hero.Skills.escort(4, "Lela Escort", 1, 2, 1))
    "E": App.KL.Game.Skill.new("Detain", 617, 16, 4, App.KL.Game.Hero.Lela.hero.Skills.detain(4, 2, 1))
  }

  let on_stepped = none

  App.KL.Game.Hero.new(name, draw, picture, max_hp, max_ap, skills, on_stepped)


App.KL.Game.Hero.Lela.hero.Skills.restore(range: I32, heal: I32, ap_cost:I32): App.KL.Game.Effect<Unit>
  App.KL.Game.Effect { 
    get target  = App.KL.Game.Effect.coord.get_target
    get center  = App.KL.Game.Effect.coord.get_center
    App.KL.Game.Effect.indicators.list(App.KL.Game.Indicator.blue, [center])
    App.KL.Game.Effect.indicators.list(App.KL.Game.Indicator.green, [target])
    App.KL.Game.Effect.common.heal.single(range, heal, ap_cost)
  }

App.KL.Game.Hero.Lela.hero.Skills.escort(range: I32, id: String, duration: I32, amount: I32, ap_cost: I32): App.KL.Game.Effect<Unit>
  App.KL.Game.Effect { 
    get target  = App.KL.Game.Effect.coord.get_target
    get center  = App.KL.Game.Effect.coord.get_center
    App.KL.Game.Effect.indicators.list(App.KL.Game.Indicator.blue, [center])
    App.KL.Game.Effect.indicators.list(App.KL.Game.Indicator.green, [target])
    App.KL.Game.Effect.common.shield.single(range, id, duration, amount, ap_cost)
  }

App.KL.Game.Hero.Lela.hero.Skills.detain(range: I32, dmg: I32, ap_cost: I32): App.KL.Game.Effect<Unit>
  App.KL.Game.Effect { 
    get target  = App.KL.Game.Effect.coord.get_target
    get center  = App.KL.Game.Effect.coord.get_center
    App.KL.Game.Effect.indicators.list(App.KL.Game.Indicator.blue, [center])
    App.KL.Game.Effect.indicators.list(App.KL.Game.Indicator.red, [target])
    App.KL.Game.Effect.common.dmg.single(range, dmg, ap_cost)
  }
