Kind.Parser.i32: Parser(Kind.Term)
  Parser {
    get init = Kind.Parser.init;
    Kind.Parser.spaces;
    get intx = Parser.int;
    Parser.text("#32");
    let term = Kind.Term.ref("Int.to_i32");
    let term = Kind.Term.app(term, Kind.Term.nat(natx));
    get orig = Kind.Parser.stop(init);
    return Kind.Term.ori(orig, term);
  }
