App.Kaelin.Coord.path(
  coord: App.Kaelin.Coord,
  goal: App.Kaelin.Coord
): Maybe<List<App.Kaelin.Coord>>

  path_min = App.Kaelin.Coord.Path.minimum(coord, goal)
  case path_min {
    none: 
      none
    some: 
      if (App.Kaelin.Coord.eql(path_min.value, goal)) then
        some([path_min.value])
      else
        ls = App.Kaelin.Coord.path(path_min.value, goal)
        Maybe.extract!(ls, Maybe<List<App.Kaelin.Coord>>, none, (a) some(path_min.value & a))
  }
