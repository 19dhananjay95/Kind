App.Kaelin.Tile.creature.change_hp(
  change: I32,
  creature: App.Kaelin.Creature
): App.Kaelin.Creature

  open creature
  open creature.hero as hero
  case I32.lte(creature.hp, 0) { //I32
    true: 
      creature
    false:
      def new_hp = I32.min(I32.add(creature.hp, change), hero.max_hp)
      creature@hp <- new_hp
      }
