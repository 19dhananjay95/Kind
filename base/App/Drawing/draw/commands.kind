App.Drawing.draw.commands(cmds:List<App.Drawing.Commands>, img:VoxBox): VoxBox
  for cmd in cmds with img:
    open cmd
    case cmd.tool {
      pencil:
        App.Drawing.draw.line(cmd.x0, cmd.y0, cmd.x1 ,cmd.y1, cmd.z, cmd.size, cmd.col, img)
      square:
        let img = App.Drawing.draw.line(cmd.x0, cmd.y0, cmd.x1 ,cmd.y1, cmd.z, cmd.size, cmd.col, img)
      img
    }default img  
    img
  