Avl.show<K: Type, V: Type>(
  show_key: K -> String,
  show_val: V -> String,
  map     : Avl<K,V>
): String
  let show_pair = Pair.show<K,V>(show_key, show_val)
  let kvs       = Avl.to_list<K,V>(map)
  let kvs       = List.map<Pair<K,V>, String>(show_pair, kvs)
  List.show<String>(String.show, kvs)