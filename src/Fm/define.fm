Fm.define(
  file: String,
  code: String,
  orig: Pair(Nat,Nat),
  name: Fm.Name,
  term: Fm.Term,
  type: Fm.Term,
  isct: Bool,
  done: Bool,
  defs: Fm.Defs,
): Fm.Defs
  //let skip = Debug.log<_>(String.flatten(["define: ", name]), (x) Unit.new);
  let stat = if done then Fm.Status.done else Fm.Status.init;
  Fm.set<_>(name, Fm.Def.new(file, code, orig, name, term, type, isct, stat), defs)