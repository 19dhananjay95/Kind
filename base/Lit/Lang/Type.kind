Lit.Lang.Type: String
`
type Bits { 
  e
  o{pred: Bits}
  i{pred: Bits}
}

type Nat { 
  zero
  succ{pred: Nat}
}

type Bool { 
  true
  false
}

type Unit { 
  new
}

type Cmp { 
  ltn
  eql
  gtn
}

// TODO use Bits.e instead
type Maybe.Bits {
  none
  some{val: Bits}
}

type BitsMap {
  new
  tie{val: Maybe.Bits, lft: BitsMap, rgt: BitsMap}
}

type Pair.BitsMap {
  new{fst: BitsMap, snd: BitsMap}
}

type Pair.Bits {
  new{fst: Bits, snd: Bits}
}
`
