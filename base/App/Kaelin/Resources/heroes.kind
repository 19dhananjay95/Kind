// we can be heroes
App.Kaelin.Resources.heroes: Map(App.Kaelin.Hero)
  let heroes = [
    App.Kaelin.Heroes.Croni.hero,
    App.Kaelin.Heroes.Cyclope.hero,
    App.Kaelin.Heroes.Lela.hero,
    App.Kaelin.Heroes.Octoking.hero,
  ]

  List.fold!(heroes)!({}, (hero, map) 
    open hero
    map{hero.name} <- hero
  )
  
