App.KL.Game.Effect.heroes.ground_slam(
  range: I32
  radius: I32
  dmg: I32
): App.KL.Game.Effect<Unit>

  App.KL.Game.Effect { 
    get self = App.KL.Game.Effect.coord.get_center  
    let area = App.KL.Game.Effect.area.circle_without_center(self, radius)
    App.KL.Game.Effect.indicators.list(App.KL.Game.Indicator.red, area)
    App.KL.Game.Effect.area!(App.KL.Game.Effect.hp.damage_at(dmg), area)
    return unit
  }

