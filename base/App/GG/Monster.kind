type App.GG.Monster {
  minion(
    AI: App.GG.AI
    data: App.GG.Minion
  )
  slime(
    data: App.GG.Slime
    hand: App.GG.Chips
    folder: App.GG.Chips
    energy: Nat
  )
}

App.GG.Monster.to_vbox(monster: App.GG.Monster): App.GG.Animation -> U64 -> VoxBox
  case monster {
    slime: monster.data@sprite
    minion: monster.data@sprite
  }

