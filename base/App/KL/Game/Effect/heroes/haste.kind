App.KL.Game.Effect.heroes.haste(
  dmg: I32
): App.KL.Game.Effect<Unit>

  App.KL.Game.Effect {
    get target = App.KL.Game.Effect.coord.get_target
    let coords = Hexagonal.Axial.neighbour(target)
    App.KL.Game.Effect.area!(App.KL.Game.Effect.hp.damage_at(dmg), coords)
    App.KL.Game.Effect.movement.move
    
  }
