App.KL.Game.Effect.heroes.restore(
  heal: I32
  area: I32
): App.KL.Game.Effect<Unit>

  App.KL.Game.Effect { 
    get mouse = App.KL.Game.Effect.coord.get_target
    let coords = Hexagonal.Axial.range(mouse, area)
    App.KL.Game.Effect.area!(App.KL.Game.Effect.hp.heal_at(heal), coords)
    return unit
  }
