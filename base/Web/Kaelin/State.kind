type Web.Kaelin.State {
  init
  void(
    room: String
  )
  game(
    room: String
    tick: Nat
    players: Map(Web.Kaelin.Player)
    cast_status: Maybe(CastStatus)
    map: Web.Kaelin.Map,
    interface : App.EnvInfo
  )
}

type Web.Kaelin.CastStatus {
  new(
    center_pos: Web.Kaelin.Coord
    target_pos: Web.Kaelin.Coord
    range: Nat
  )
}



