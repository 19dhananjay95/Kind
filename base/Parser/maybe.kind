Parser.maybe<V: Type>(parse: Parser(V)): Parser(Maybe<V>)
  (long, idx, code) case parse(long, idx, code) as reply {
    error: Parser.Reply.value<Maybe<V>>(some(reply.val), idx, code, Maybe.none<V>),
    value: Parser.Reply.value<Maybe<V>>(reply.long, reply.idx, reply.code, Maybe.some<V>(reply.val)),
  }
