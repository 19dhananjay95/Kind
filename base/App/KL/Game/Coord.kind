App.KL.Game.Coord.eql(
  a:App.KL.Game.Coord,
  b:App.KL.Game.Coord
): Bool

  open a
  open b
  Bool.and(I32.eql(a.i,b.i), I32.eql(a.j, b.j))


App.KL.Game.Coord.axial_to_nat(
  coord: App.KL.Game.Coord
): Nat

  open coord
  i = coord.i + 100 //  I32
  i = i * 1000
  i = I32.to_u32(i)
  j = coord.j + 100
  j = I32.to_u32(j)
  sum = i + j   //U32
  U32.to_nat(sum)