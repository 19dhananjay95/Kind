App.Kaelin.Coord.path(c : App.Kaelin.Coord, goal : App.Kaelin.Coord) : Maybe<List<App.Kaelin.Coord>>
  let path_min = App.Kaelin.Coord.Path.minimum(c, goal)
  case path_min {
      none :  none
      some : if (App.Kaelin.Coord.eql(path_min.value, goal)) then
               some([path_min.value])
            else
               let ls = App.Kaelin.Coord.path(path_min.value, goal)
               Maybe.extract!(ls, Maybe<List<App.Kaelin.Coord>>, none, (a) some(path_min.value & a))
  }