App.KL.Game.Effect.caster_id.get_from_center: App.KL.Game.Effect<String>
  App.KL.Game.Effect {
    get caster = App.KL.Game.Effect.board.creature.caster
    let caster_id = Maybe {
      get caster = caster
      get player = caster@player
      return player
    } <> "no address"
    return caster_id  
  }
