App.KL.Game.Effect.heroes.gust(
  dmg: I32
  area: I32
): App.KL.Game.Effect<Unit>

  App.KL.Game.Effect { 
    get target = App.KL.Game.Effect.coord.get_target
    get self = App.KL.Game.Effect.coord.get_center
    let circle = Hexagonal.Axial.range(target, area)
    App.KL.Game.Effect.area!(App.KL.Game.Effect.hp.damage_at(dmg), circle)
    return unit
  }
