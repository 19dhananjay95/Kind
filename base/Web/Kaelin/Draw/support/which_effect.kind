// Returns the cast affecting a tile
Web.Kaelin.Draw.support.which_effect(
  coord: Web.Kaelin.Coord,
  cast_info: Web.Kaelin.CastInfo
  range_list: List<Web.Kaelin.Coord>,
) : Web.Kaelin.HexEffect
  open cast_info
  let is_in_range = List.any!(Web.Kaelin.Coord.eql(coord), range_list)
  case is_in_range {
    true: cast_info.hex_effect
    false: Web.Kaelin.HexEffect.normal
  }

    