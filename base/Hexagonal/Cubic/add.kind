// Returns the sum of two points given two coordinates
Hexagonal.Cubic.add(
  coord_a: Hexagonal.Cubic,
  coord_b: Hexagonal.Cubic
): Hexagonal.Cubic

  open coord_a
  open coord_b
  x = coord_a.x + coord_b.x // I32
  y = coord_a.y + coord_b.y // I32
  z = coord_a.z + coord_b.z // I32
  Hexagonal.Cubic.new(x, y, z)
