Parser.try<V: Type>(parse: Parser(V)): Parser(Bool)
  (err, idx, code) case parse(err, idx, code) as reply {
    error:
      new_long = Parser.ErrorAt.maybe_combine(err, some(reply.err))
      Parser.Reply.value<Bool>(new_long, idx, code, false),
    value: Parser.Reply.value<Bool>(err, idx, code, true),
  }
