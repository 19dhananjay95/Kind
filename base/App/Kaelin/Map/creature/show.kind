App.Kaelin.Map.creature.show(
  map: App.Kaelin.Map
): String

  map = NatMap.to_list!(map)
  result = ""
  for pos in map with result:
    key = pos@fst
    coord = App.Kaelin.Coord.Convert.nat_to_axial(key)
    
    tile = pos@snd
    open tile
    case tile.creature {
      none: result | ""
      some: result | App.Kaelin.Coord.show(coord) | " " | App.Kaelin.Tile.creature.show(tile.creature.value) | "\n"
    }
  result
