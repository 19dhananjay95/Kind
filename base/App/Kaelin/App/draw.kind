App.Kaelin.App.draw(
  img: VoxBox
): App.Draw<App.Kaelin.State>

(state)
  global = state@global
  stage = global@stage
  case stage {
    init: App.Kaelin.Draw.init
    draft: App.Kaelin.Draw.draft
    planning: App.Kaelin.Draw.planning(img, state)
    action: App.Kaelin.Draw.action(img, state)
  } default DOM.text("Em breve")
