// Moves the creature  at coordinate "ca" to coordinate "cb"
Web.Kaelin.Map.swap(
  ca: Web.Kaelin.Coord
  cb: Web.Kaelin.Coord
  map: Web.Kaelin.Map
) : Web.Kaelin.Map
  {new_map, creature} = Web.Kaelin.Map.pop_creature(ca, map)
  case creature {
    none: map
    some: Web.Kaelin.Map.push(cb, Either.right!!(creature.value), new_map)
  }
