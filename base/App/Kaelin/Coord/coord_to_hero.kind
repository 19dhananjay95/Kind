App.Kaelin.Coord.coord_to_hero(coord: App.Kaelin.Coord, map: App.Kaelin.Map): Maybe(App.Kaelin.Hero)
  let tile = App.Kaelin.Map.get(coord, map) <> []
  let heroi = none
    for entity in tile with heroi:
      case entity {
      background: heroi
      creature: some(entity.hero)              
    }
    heroi
      
