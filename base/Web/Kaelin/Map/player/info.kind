Web.Kaelin.Map.player.info(address: String, map:Web.Kaelin.Map): Maybe<Pair<Web.Kaelin.Coord,Web.Kaelin.Creature>>
  coord = Web.Kaelin.Map.player.to_coord(address,map)
  case coord{
    none: none
    some: 
      let tile = Web.Kaelin.Map.get(coord.value, map)
      case tile {
        none: none
        some: 
          open tile.value as tile
          case tile.creature as creature{
            none: none
            some: some({coord.value,creature.value})
          }
      }
  }