Monoid.product<A: Type, B: Type>(MA: Monoid(A), MB: Monoid(B)): Monoid(Pair(A,B))
  Monoid.new(
  // The product type
  Pair(A,B),

  // The binary operation
  (ab1, ab2)
  let {a1, b1} = ab1
  let {a2, b2} = ab2
  {MA@op(a1, a2), MB@op(b1, b2)},

  // The neuter element
  {MA@e, MB@e},

  // Associativity
  (ab1, ab2, ab3)
  let {a1, b1} = ab1
  let {a2, b2} = ab2
  let {a3, b3} = ab3
  ?assoc,
  ?left,
  ?right,
  )
