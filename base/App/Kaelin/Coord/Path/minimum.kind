//Minimum path between 2 coords
App.Kaelin.Coord.Path.minimum(
  origin: App.Kaelin.Coord,
  goal: App.Kaelin.Coord
): Maybe(App.Kaelin.Coord)

neighborhood = App.Kaelin.Coord.Path.moore(origin)
sort = ((x, y) 
  case y {
    none: 
      some(x)
    some: 
      if App.Kaelin.Coord.Path.is_shorter(goal, x, y.value) then
        some(x)
      else 
        y
    }) :: App.Kaelin.Coord -> Maybe(App.Kaelin.Coord) -> Maybe(App.Kaelin.Coord)
  List.foldr!!(none, sort, neighborhood)
