App.KL.Game.Effect.status.dot.apply(
  caster_id: String
  id: String
  duration: I32
  amount: I32
): App.KL.Game.Effect.At<I32>
  (pos)

  App.KL.Game.Effect {
    App.KL.Game.Effect.caster_id.add(caster_id)
    if duration >? 0 then
      App.KL.Game.Effect.hp.damage_at(amount, pos)
    else
      App.KL.Game.Effect {return +0#32}
  }