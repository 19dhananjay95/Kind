App.Kaelin.Skill.restore(
  key: U16,
  value: I32,
): App.Kaelin.Effect<Unit>

  App.Kaelin.Effect {
    get target = App.Kaelin.Effect.coord.get_target
    get self = App.Kaelin.Effect.coord.get_center
    App.Kaelin.Effect.hp.heal_at(value, target)
    App.Kaelin.Effect.ap.cost(key, self)
    return unit
  }
