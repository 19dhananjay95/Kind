App.KL.Game.Effect.ap.verify(
  cost: I32
): App.KL.Game.Effect<Unit>

  (center, target, board)
  let creature = App.KL.Game.Board.Creature.get(center, board)
  without creature: App.KL.Game.Effect.Result.err<Unit>
  if I32.gtn(cost, creature@ap) then 
    App.KL.Game.Effect.Result.err<Unit>
  else
    App.KL.Game.Effect.Result.new<Unit>(unit, board, Hexagonal.Axial.BBT.new!)
