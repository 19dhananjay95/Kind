Parser.maybe<V: Type>(parse: Parser(V)): Parser(Maybe<V>)
  (err, idx, code) case parse(err, idx, code) as reply {
    error: Parser.Reply.value<Maybe<V>>(some(reply.err), idx, code, Maybe.none<V>),
    value: Parser.Reply.value<Maybe<V>>(reply.err, reply.idx, reply.code, Maybe.some<V>(reply.val)),
  }
