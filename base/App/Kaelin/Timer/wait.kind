App.Kaelin.Timer.wait(frame : Nat,
                     timer : List<App.Kaelin.Timer>,
                     state :  App.Kaelin.State 
                     ) : App.Kaelin.State
  
  let {xs, ys} = List.take_while.go!((x) open x Nat.ltn(x.time, frame), timer)
  def foldr = List.foldr(App.Kaelin.Timer, App.Kaelin.State -> App.Kaelin.State)
  App.Kaelin.Timer.set_timer(ys, foldr((x) x, 
    (x, f) open x Function.comp(_, _, _, f, x.action), xs, state))