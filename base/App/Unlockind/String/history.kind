//Função que acumula os resultados anteriores pra mostrar em histórico
App.Unlockind.String.history(
  game: App.Unlockind.play
  input: List<String>
  results: List<String>
): List<Pair<List<String>, List<String>>>

  open game
  let history = game@history
  let new_try = {input, results}
  case history { 
    nil:
      [new_try]
    cons:
      new_try & history
  }
  
