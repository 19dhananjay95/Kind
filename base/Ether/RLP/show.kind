Ether.RLP.show(r: Ether.RLP): String
  case r {
    data:
      let ret = Bytes.to_hex(r.value)
      case ret {
        nil:
          `""`
        cons:
          "0x"|ret
      }
    node:
      case r.child {
        nil:
          "[]"
        cons:
        let ret = ""
        for child in r.child.tail with ret:
          ret|", "|Ether.RLP.show(child)
        "["|Ether.RLP.show(r.child.head)|ret|"]"
      }
  }
