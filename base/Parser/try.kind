Parser.try<V: Type>(parse: Parser(V)): Parser(Bool)
  (long, idx, code) case parse(long, idx, code) as reply {
    error:
      new_long = Parser.ErrorAt.maybe_combine(long, some(reply.val))
      Parser.Reply.value<Bool>(new_long, idx, code, false),
    value: Parser.Reply.value<Bool>(long, idx, code, true),
  }
